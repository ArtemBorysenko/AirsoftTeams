    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "express-socket.io-session": "^1.3.5",
    "express-validator": "^6.3.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.1",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
